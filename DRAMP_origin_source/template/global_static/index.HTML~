function Addfavorite()
{
	try
    {
        window.external.addFavorite('http://www.dramp.com','dramp');
    }
    catch (e)
    {
        try
        {
            window.sidebar.addPanel('dramp','http://www.dramp.com',"");
        }
        catch (e)
        {
            alert("Fail to add Favorite,please use the key combination 'Ctrl+D'.");
        }
    }
}
</script>
